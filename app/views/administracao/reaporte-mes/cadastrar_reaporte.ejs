<%- include('../../includes/menu_header'); %>
<div>
	<%- include('../submenu'); %>
</div>

<%if(usuario.nivel == 1){ %>
<div class="container-fluid">
	<div class="w100 text-center">
		<div class="bold f25 text-center white-text margin-b-10">Adicionar Reaporte</div>


		<form method="POST" action="" enctype="multipart/form-data">

			<div class="row">

				<div class="form-group col-md-12">
					<div class="row">
						<label class="white-text col-sm-2" for="mes">Mês</label>
						<div class="col-sm-10">
							<input required="true" class="validate form-control datepicker data-sem-hora" type="text" name="data">
						</div>
					</div>
				</div>
			</div>


			<button data-href="/sistema/administracao/reaporte_mes/confirmar_cadastro" class="btn primary ajax-submit-open-modal margin-b-10" data-modal="#modalinfo">
				Salvar <i class="far fa-save-o" aria-hidden="true"></i>
			</button>

		</form>
	</div>

</div>

<%}else{%>
<div class="white-text text-center f25">Você não possui permissão para acessar esse conteúdo!</div>
<%}%>