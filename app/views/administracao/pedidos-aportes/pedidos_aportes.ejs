<div class="col s12 center padding-0">
	<div class="bold f25 margin-l-20 border-bottom-gold margin-b-10">
		Administração de Aprovação de Aporte
		<i class="fas fa-arrow-alt-circle-up right margin-r-20 gold-text"></i>
	</div>
	
	
	<%if(usuario.nivel == 4 || usuario.nivel == 5){ %>


	<div class="col s12">

		<table class="table-full-width tabela_filtrada">
			<thead class="gold-text">
				<tr>
					<th>Nome</th>
					<th>Perfil</th>
					<th>Qtd. BTC</th>
					<th>Data Cadastro</th>
				</tr>
			</thead>
			<tbody>

				<%
				if (typeof data.pedido_aporte != 'undefined' && data.pedido_aporte.length > 0) {
					for (var i = 0; i < data.pedido_aporte.length; i++) { %>

					<tr class="border-bottom-gold">
						<td>
							<a class="white-text ajax-load break-all" href="<%= data.link_sistema%>/administracao/pedido-aporte/editar/<%= data.pedido_aporte[i].id %>">
								<%= data.pedido_aporte[i].nome %>
							</a> 
						</td>
						<td>
							<a class="white-text ajax-load break-all" href="<%= data.link_sistema%>/administracao/pedido-aporte/editar/<%= data.pedido_aporte[i].id %>">
								<%= data.pedido_aporte[i].plano %> 
							</a>
						</td>
						<td>
							<a class="white-text ajax-load" href="<%= data.link_sistema%>/administracao/pedido-aporte/editar/<%= data.pedido_aporte[i].id %>">
								<%= data.pedido_aporte[i].qtd_btc %>
							</a>
						</td>
						<td>
							<a class="white-text ajax-load" href="<%= data.link_sistema%>/administracao/pedido-aporte/editar/<%= data.pedido_aporte[i].id %>">
								<span class="hide"><%= data.pedido_aporte[i].data_table_filtro %></span>
								<%= data.pedido_aporte[i].data_cadastro %>
							</a>
						</td>

					</tr>

					<%} 
				}else{%>
				<tr>
					<td colspan="5" class="center">Não há Aprovações de Aportes no momento</td>
				</tr>
				<%}%>
			</tbody>

		</table>
	</div>





	<%}else{%>
	<div class="card light">Você não possui permissão para acessar esse conteúdo</div>
	<%}%>

</div>